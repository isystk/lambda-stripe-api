{
  "name": "stripe-subscription-api-frontend",
  "description": "stripe-subscription-api-frontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "PORT=9000 NODE_ENV=development next",
    "start": "PORT=9000 NODE_ENV=production next start",
    "build": "NODE_ENV=production next build && next export",
    "test": "jest ",
    "test:coverage": "jest --collect-coverage",
    "lint": "eslint --ext .ts,.tsx .",
    "lintfix": "eslint --fix --ext .ts,.tsx .",
    "format": "prettier -w 'src/**/*.{ts,tsx}'",
    "fix": "yarn format && yarn lint",
    "deploy:production": "now -t $VERCEL_TOKEN -c --prod",
    "deploy:preview": "now -t $VERCEL_TOKEN -c",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,sass,scss}": [
      "stylelint --fix"
    ]
  },
